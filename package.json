      "git add"
    ]
  },
  "main": "index.js",
  "name": "@containator/stylelint-config-base",
  "peerDependencies": {
    "stylelint": "^14.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:com:containator/stylelint-config-base.git"
  },
  "scripts": {
    "lint:fix": "eslint . --ext .js --fix",
    "prepare": "husky install"
  },
  "version": "1.0.0"
}
