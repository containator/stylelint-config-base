{
  "author": "Maxim A.",
  "devDependencies": {
    "@containator/eslint-config-base": "^1.0.0",
    "eslint": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
